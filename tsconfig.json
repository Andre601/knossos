{
	"compilerOptions": {
		"paths": {
			"$assets/*": ["src/assets/*"],
			"$components/*": ["src/components/*"],
			"$stores/*": ["src/stores/*"],
			"$styles/*": ["src/styles/*"],
			"$generated/*": ["generated/*"],
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"]
		},
		"resolveJsonModule": true,
		"esModuleInterop": true
	},
	"extends": "./.svelte-kit/tsconfig.json"
}
